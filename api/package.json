{
  "name": "api",
  "version": "0.0.1",
  "description": "API package for Restaurant Ordering",
  "main": "index.js",
  "scripts": {
    "docs:build": "apidoc -i src/controllers -o docs && mv docs dist/docs",
    "clean": "rm -rf docs dist",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "start": "concurrently -k -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"yarn watch-ts\" \"nodemon ./dist/index.js\"",
    "watch-ts": "tsc -w && yarn docs:build",
    "build": "yarn clean && tsc && yarn docs:build",
    "serve": "NODE_PATH=dist/ node ./dist/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "ts-node -r tsconfig-paths/register ./src/scripts/runMigrations.ts",
    "migrate:empty": "yarn typeorm migration:create",
    "migrate:generate": "yarn typeorm migration:generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/neuatech/restaurantordering-web.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/neuatech/restaurantordering-web#readme",
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@hapi/joi": "^16.1.7",
    "@koa/router": "^8.0.2",
    "@types/hapi__joi": "^16.0.3",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa": "^2.0.52",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^12.12.7",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "pg": "^7.12.1",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "apidoc": "^0.18.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-typescript": "^6.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2"
  }
}
